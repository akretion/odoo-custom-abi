        <template id="assets_backend" name="pos_delivery assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/pos_delivery/static/src/js/pos_delivery.js"></script>
            </xpath>
        </template>
        <template id="index_pos_delivery" name="POS Index delivery" inherit_id="point_of_sale.index">
